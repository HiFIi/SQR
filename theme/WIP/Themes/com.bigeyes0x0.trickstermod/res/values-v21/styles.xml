<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBarCustomBg" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:background">#ff272048</item>
    </style>
    
    <style name="GroupLabel" parent="@android:style/TextAppearance.Material.Medium">
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lines">1</item>
    </style>
    
    <style name="SectionHeader" parent="@android:style/Widget.Material.TextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:layout_marginTop">8.0dip</item>
        <item name="android:layout_marginBottom">8.0dip</item>
        <item name="android:textAllCaps">true</item>
    </style>
    
    <style name="Theme.Holo" parent="@android:style/Theme.Material">
        <item name="android:actionBarStyle">@style/action_bar_style</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#ff141025</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
        <item name="android:colorPrimary">#272048</item>
        <item name="android:colorPrimaryDark">#141025</item>
        <item name="android:colorAccent">#272048</item>
        <item name="android:navigationBarColor">@android:color/black</item>
    </style>
    
</resources>
